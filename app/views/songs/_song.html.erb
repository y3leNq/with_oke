<ol class="list-group mt-2">
  <li class="list-group-item align-items-start">
    <%= image_tag song["artworkUrl100"], class: "float-start me-5" %>
    <div>
      <h4 class="mt-3">
        <%= song["trackName"] %>
      </h4>
      <p>
        <%= song["artistName"] %>
      </p>
      <audio controls src="<%= song["previewUrl"] %>"></audio>
      <%= link_to 'Add', new_song_path(track_id: song["trackId"]), class: "btn btn-primary" %>
    </div>
  </li>
</ol>
